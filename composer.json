{
    "name": "rohit-raj-verma/pimcore-validation-bundle",
    "description": "Pimcore bundle to add complex field validation rules in class definitions",
    "type": "pimcore-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Rohit Raj Verma",
            "email": "rohitrajv5@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "pimcore/pimcore": "^11.0",
        "pimcore/admin-ui-classic-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PimcoreValidationBundle\\": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "PimcoreValidationBundle\\PimcoreValidationBundle"
            ]
        }
    }
}
